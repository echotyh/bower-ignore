{
  "args": {
    "bowerDir": "bower_components"
  },
  "in": {
    "bootstrap": {
      "ignore": ["*", "dist/css/*.map"],
      "unignore": ["dist/*"]
    },
    "font-awesome": {}
  },
  "out": [
    "bower_components/bootstrap/*",
    "!bower_components/bootstrap/dist/*",
    "bower_components/bootstrap/dist/css/*.map"
  ]
}
